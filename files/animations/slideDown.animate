/* 
  Name:			Slide Down
  Type:			Entrance
  Description:	Slide Down Effect
  Version:		2.0	
  locale:		{"zh-Hans":"向下滑动", "en-us":"Slide Down"}
 */
 
@keyframes slideDown {
	0% {
		transform: translateY(-100%);
	}
	50%{
		transform: translateY(8%);
	}
	65%{
		transform: translateY(-4%);
	}
	80%{
		transform: translateY(4%);
	}
	95%{
		transform: translateY(-2%);
	}			
	100% {
		transform: translateY(0%);
	}		
}